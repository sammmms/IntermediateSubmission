<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000" motion:motionInterpolator="cubic(0.2,0.2,0.8,0.62)">
        <KeyFrameSet></KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/logo"
            android:layout_width="160dp"
            android:layout_height="160dp"
            motion:transitionEasing="cubic(0.2,0.2,0.77,0.79)"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="4dp"
            android:layout_marginTop="200dp"></Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/logo"
            android:layout_width="160dp"
            android:layout_height="160dp"
            motion:layout_constraintTop_toTopOf="parent"></Constraint>
    </ConstraintSet>
</MotionScene>